{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": [
      "node",
      "@cloudflare/workers-types"
    ],
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  }
}
