{
  "name": "telecommut.com",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "typecheck": "astro check",
    "lint": "eslint \"src/**/*.{ts,tsx,astro}\" \"scripts/**/*.mjs\" \"tests/**/*.ts\" drizzle.config.ts eslint.config.mjs vitest.config.ts",
    "test": "vitest run",
    "db:check": "node scripts/check-migrations.mjs",
    "qa:role-cutover": "node scripts/check-role-cutover.mjs",
    "qa:no-legacy-type-writes": "node scripts/check-no-legacy-type-writes.mjs",
    "qa:wave6-baseline": "node scripts/check-wave6-baseline.mjs",
    "qa:wave6-social": "node scripts/check-wave6-social.mjs",
    "qa:delta-report": "node scripts/verify-delta-report.mjs",
    "qa:delta-summary": "node scripts/summarize-delta-report.mjs",
    "qa:cutover-smoke": "node scripts/cutover-smoke.mjs",
    "qa:data": "node scripts/data-qa.mjs",
    "ci:check": "npm run typecheck && npm run lint && npm run test && npm run db:check && npm run qa:role-cutover && npm run qa:no-legacy-type-writes && npm run qa:wave6-baseline",
    "db:generate": "drizzle-kit generate",
    "db:migrate:sqlite": "node scripts/migrate-sqlite.mjs",
    "db:migrate:d1:local": "wrangler d1 migrations apply telecommut-db --local",
    "db:migrate:d1:remote": "wrangler d1 migrations apply telecommut-db --remote",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.12",
    "@astrojs/react": "^4.4.2",
    "@tailwindcss/vite": "^4.2.1",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "astro": "^5.17.1",
    "better-auth": "^1.4.19",
    "better-sqlite3": "^12.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "lucide-react": "^0.575.0",
    "radix-ui": "^1.4.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwind-merge": "^3.5.0",
    "tailwindcss": "^4.2.1"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@cloudflare/workers-types": "^4.20260302.0",
    "@eslint/js": "^10.0.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.3.0",
    "astro-eslint-parser": "^1.3.0",
    "dotenv": "^17.3.1",
    "drizzle-kit": "^0.31.9",
    "eslint": "^10.0.1",
    "eslint-plugin-astro": "^1.6.0",
    "globals": "^17.3.0",
    "shadcn": "^3.8.5",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1",
    "vitest": "^4.0.18",
    "wrangler": "^4.67.1"
  }
}
