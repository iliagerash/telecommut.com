---
import PublicLayout from "@/layouts/PublicLayout.astro";

export const prerender = false;

const { id } = Astro.params;
---

<PublicLayout title={`Resume ${id ?? ""}`} description="Resume detail page.">
  <h1 class="text-3xl font-semibold tracking-tight">Resume Detail</h1>
  <p class="mt-3 text-muted-foreground">Resume id: <code>{id}</code></p>
  <p class="mt-2 text-muted-foreground">Visibility rules will be finalized in W3-2.</p>
</PublicLayout>
